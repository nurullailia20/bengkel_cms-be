generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model user {
  id            String    @id @default(cuid())
  email         String    @unique
  password      String
  name          String
}


model customer {
  id                    String          @id @default(cuid())
  name                  String
  vehicle               String
  police_number         String
  phone_number          String
  total_point           Int?
}
